    "autobind-decorator": "^1.4.3",
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.1",
    "better-npm-run": "0.0.15",
    "co": "^4.6.0",
    "cross-env": "^5.0.1",
    "crypto": "0.0.3",
