{
  "name": "api-rest",
  "version": "1.0.0",
  "description": "the api of store project",
  "main": "server.js",
  "scripts": {
    "start": "better-npm-run start-online",
    "dev": "better-npm-run start-dev"
  },
  "betterScripts": {
    "start-dev": {
      "command": "supervisor ./endpoint.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start-online": {
      "command": "node ./endpoint.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/burning0xb/jslight-api.git"
  },
  "keywords": [
    "koa2",
    "wechat"
  ],
  "author": "wangrui",
  "license": "ISC",
  "dependencies": {
    "ali-oss": "^4.8.0",
